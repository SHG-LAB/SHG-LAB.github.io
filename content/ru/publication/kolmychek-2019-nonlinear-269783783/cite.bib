 issn = {1551-7616; 0094-243X},
 journal = {AIP Conference Proceedings},
 language = {english},
 pages = {030006-1--030006-4},
 title = {Nonlinear Optics of Resonant Metamaterials},
 volume = {2136},
 year = {2019}
}
