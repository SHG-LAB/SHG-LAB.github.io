@article{kolmychekIssledovanieMagnitoopticheskogoOtklika2015,
 abstract = {Экспериментально исследованы резонансные оптические эффекты в упорядоченных планарных дву- мерных массивах золотых нанодисков, расположенных в диэлектрической магнитной пленке. Частотно- угловые спектры пропускания демонстрируют особенности, связанные с возбуждением локальных по- верхностных плазмонов и решеточных плазмон-поляритонов в золотых наночастицах, а также волно- водных мод в слое магнитного диэлектрика. Для данных структур обнаружено резонансное усиление линейного интенсивностного магнитооптического эффекта в проходящем излучении, наблюдающееся в спектральной окрестности возбуждения решеточного плазмонного резонанса и волноводной моды в структуре.},
 author = {Колмычек, И. А. and Шайманов, А. Н. and Барышев, А. В. and Мурзина, Т. В.},
 doi = {10.7868/S0370274X1513010X},
 issn = {0370-274X},
 journal = {Письма В \dqжурнал Экспериментальной И Теоретической Физики\dq},
 language = {en},
 month = {June},
 number = {1},
 pages = {50--55},
 title = {Исследование магнитооптического отклика двумерных магнитных плазмонных структур на основе золотых нанодисков в слое феррит-граната},
 volume = {102},
 year = {2015}
}
