 doi = {10.1134/S0367676507010061},
 issn = {0367-6765},
 journal = {Известия Российской академии наук. Серия физическая},
 language = {russian},
 number = {1},
 pages = {29--31},
 title = {Наноструктурированные одномерные и трехмерные магнитофотонные кристаллы на основе пористого кремния и искусственных опалов},
 volume = {71},
 year = {2007}
}
