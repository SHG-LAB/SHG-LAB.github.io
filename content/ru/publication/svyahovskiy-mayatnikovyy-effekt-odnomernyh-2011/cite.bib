@inproceedings{svyahovskiyMayatnikovyyEffektOdnomernyh2011,
 abstract = {Фотонные кристаллы – периодические структуры с периодом порядка длины волны видимого света – вот уже более 20 лет являются объектом интенсивного исследования и находят применение в различных приложениях. Простейший одномерный ФК состоит из двух типов чередующихся диэлектрических слоев с различными показателями преломления. Одна из возможных геометрических схем изучения такого ФК – геометрия Лауэ. Теоретически было показано, что в случае геометрии Лауэ квазимонохроматический импульс света, распространяющийся в ФК, при определенных условиях претерпевает два типа деления: пространственное и временное.},
 address = {МГУ Москва, Россия},
 author = {Свяховский, С. Е. and Скорынин, А. А. and Манцызов, Б. И. and Мурзина, Т. В.},
 booktitle = {Материалы XIII Всероссийской Школы-семинара \flqqфизика И Применение Микроволн\frqq (волны-2011)},
 language = {en},
 title = {Маятниковый эффект в одномерных фотонных кристаллах},
 year = {2011}
}
