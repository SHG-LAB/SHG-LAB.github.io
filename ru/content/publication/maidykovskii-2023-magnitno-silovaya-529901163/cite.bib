@article{maidykovskii2023magnitno-silovaya529901163,
 annote = {Методами магнитно-силовой, поляризационно-оптической и нелинейно-оптической микроскопии изучены магнитные свойства приповерхностного слоя эпитаксиальной пленки феррита-граната, легированного висмутом, со слабой одноосной анизотропией. Показано, что вблизи свободной поверхности пленки наблюдается значительная модуляция интенсивности оптической второй гармоники вдоль полосовых доменов. Это обусловлено периодическими искажениями структуры полосовых доменов вблизи поверхности пленки и наличием приповерхностных несквозных доменов, обнаруженных с помощью магнитно-силовой микроскопии (МСМ). Полученная серия МСМ-изображений пленки в планарном магнитном поле с напряженностью, изменяющейся от −400 Эдо 400 Э, показывает квазистатический процесс зарождения несквозных доменов, периодически расположенных вдоль полосовых.},
 author = {Мамонов, Е. А. and Новиков, В. Б. and Майдыковский, А. И. and Темирязева, М. П. and Темирязев, А. Г. and Федорова, А. А. and Логунов, М. В. and Никитов, С. А. and Мурзина, Т. В.},
 issn = {0044-4510},
 journal = {Журнал экспериментальной и теоретической физики},
 language = {russian},
 pages = {41--49},
 title = {Магнитно-силовая и нелинейно-оптическая микроскопия приповерхностной доменной структуры эпитаксиальной пленки феррита-граната},
 volume = {163},
 year = {2023}
}
