	number = {2},
	urldate = {2025-03-15},
	journal = {Journal of the Optical Society of America B: Optical Physics},
	author = {Kolmychek, I. A. and Krutyanskiy, V. L. and Murzina, T. V. and Sapozhnikov, M. V. and Karashtin, E. A. and Rogov, V. V. and Fraerman, A. A.},
