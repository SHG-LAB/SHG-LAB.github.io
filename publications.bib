@article{miteteloOrganicElectromechanicalcavityEmitting2023,
	title = {An organic electro-mechanical-cavity emitting efficiently tunable, continuous-wave-pumped non-linear-optical-modes},
	volume = {11},
	issn = {2195-1071},
	
	doi = {10.1002/adom.202201635},
	abstract = {Organic polar crystals are a promising material platform for achieving unique nonlinear nanophotonic properties. Mainly, tunable second-harmonic generation (SHG) with continuous-wave (CW) pump sources is considered as breakthrough technology for optical switching, actuation, sensing, chip-integrated coherent light sources, and low-threshold tunable microlasers. Here, the discovery of tunable CW SHG in a polar (non-centrosymmetric) electro-mechanical microcrystal cavity of 4-(4-(methylthio)phenyl)-2,6-di(1H-pyrazol-1-yl)pyridine (UOH1) is demonstrated. As a result of total internal reflection in mirror-like light-reflecting facets and its high second-order nonlinear susceptibility, the octahedron-shaped cavities exhibit both bright femtosecond and CW-pumped SHG with coherent whispering gallery mode (WGM) and bow-tie-like (BT) optical modes excited in its spectrum. Moreover, for an external dc electric field of about 23 kV cm‚àí1 the microcavity displays 0.18 nm resonant wavelength peak shift in the second harmonic signal due to electrostriction (strain effect).},
	language = {en},
	number = {13},
	urldate = {2025-03-15},
	journal = {Advanced Optical Materials},
	author = {Mitetelo, Nikolai and Ravi, Jada and Mondal, Subrata and Venkataramudu, Uppari and Mamonov, Evgeniy and Popov, Mikhail and Maydykovskiy, Anton and Vaitheeswaran, Ganapathy and Murzina, Tatiana and Chandrasekar, Rajadurai},
	month = jul,
	year = {2023},
	pages = {2201635},
}

@article{romashkinaCollectiveLatticePlasmonic2023d,
	title = {Collective lattice and plasmonic resonances in the enhancement of circular dichroism in disk‚Äìrod metasurface},
	volume = {133},
	issn = {1089-7550; 0021-8979},
	
	doi = {10.1063/5.0124798},
	abstract = {Compact planar photonic elements serving for efficient control over the polarization of light are of paramount importance in photonics. Here, we propose a design of a chiral periodic metasurface based on plasmonic nanodisks and nanorods arranged asymmetrically in a unit cell. Using the finite-difference time-domain analysis, we show that the collective lattice resonance harnessed by the diffraction coupling of the plasmonic unit cells is the heart of the revealed resonant 38\% circular dichroism effect. The circular dichroism enhancement of the considered structure is improved using the deep-learning-assisted optimization of the metasurface design.},
	language = {en},
	number = {4},
	urldate = {2025-03-15},
	journal = {Journal of Applied Physics},
	author = {Romashkina, A. M. and Novikov, V. B. and Murzina, T. V.},
	month = jan,
	year = {2023},
	note = {Place: United States
Publisher: AIP Publishing},
	pages = {43103},
}

