@article{kolmychekSecondHarmonicGeneration1998,
	title = {Second harmonic generation in media with inhomogeneous magnetization},
	volume = {1874},
	copyright = {https://www.elsevier.com/tdm/userlicense/1.0/},
	issn = {1551-7616; 0094-243X},
	
	doi = {10.1016/S0304-8853(98)00267-4},
	abstract = {Anisotropy of the nonlinear magnetooptical (MO) response in regular arrays of Py antidots and {\textbackslash}flqq–°{\textbackslash}frqq-, {\textbackslash}flqqU{\textbackslash}frqq- and {\textbackslash}flqqO{\textbackslash}frqq- shaped Py nanoelements is studied by means of the MO Kerr effect at the second harmonic (SH) wavelength. We demonstrate that the values of the MO effect and coercivity depend substantially on the azimuthal orientation of the antidots array relatively to the external magnetic field. In the arrays of {\textbackslash}flqq–°{\textbackslash}frqq-, {\textbackslash}flqqU{\textbackslash}frqq- and {\textbackslash}flqqO{\textbackslash}frqq- shaped Py nanoelements the circular dichroism (CD) of the SH response was observed due to the extrinsic chirality of the samples. We demonstrate that the averaged over the azimuthal angle value of the CD depends on the applied to the structure magnetic field.},
	language = {en},
